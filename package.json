{
  "name": "swissparl",
  "version": "0.4.0",
  "description": "A typed javascript library for interacting with the swiss parlament open data webservices",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "lint": "npx eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "keywords": [
    "swiss",
    "parlament",
    "odata",
    "webservice"
  ],
  "author": "benggy",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelschoenbaechler/swissparl.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "prettier": "2.7.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@odata/client": "^2.20.2"
  }
}
