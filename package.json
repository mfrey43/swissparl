{
  "name": "swissparl",
  "version": "0.6.1",
  "description": "A typed javascript library for interacting with the swiss parlament open data webservices",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "lint": "npx eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "odata:generate": "npx odata2ts swissparl && npm run format"
  },
  "keywords": [
    "swiss",
    "parlament",
    "odata",
    "webservice"
  ],
  "author": "benggy",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelschoenbaechler/swissparl.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@odata2ts/odata2ts": "^0.38.1",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "eslint": "^9.11.1",
    "prettier": "2.7.1",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.8.0"
  },
  "dependencies": {
    "@odata/client": "^2.21.8",
    "@odata2ts/odata-core": "^0.6.0"
  }
}
